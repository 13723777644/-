/**index.wxss**/
@import "/welCropper/welCropper.wxss";
/**index.wxss**/
page {
  background-color: #F3F3F3
}

swiper {
  height: 300rpx;
}
bu{
  padding-top: 15rpx;
}
.slide-image {
  width: 100%;
  height: 300rpx;
}

.button_layer {
  height: 500rpx;
  width: 93%;
  margin: 0 auto;
  border-radius: 20rpx;
  display: flex;
  flex-wrap: wrap;
  position: relative;
  box-sizing: border-box;
  background-color: #fff;
  margin-top: 30rpx;
}

.button_item {
  box-sizing: border-box;
  width: 50%;
  padding: 20rpx 0 10rpx 0; 
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.item1 {
  border-right: 2rpx solid #E6E6E6;
  border-bottom: 2rpx solid #E6E6E6;
}

.item2 {
  border-bottom: 2rpx solid #E6E6E6;
}

.item3 {
  border-right: 2rpx solid #E6E6E6;
}

.item5 {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 999;
  margin-top: -100rpx;
  margin-left: -100rpx;
}

.item_img {
  width: 80rpx;
  height: 80rpx;
}

.item5_img {
  width: 200rpx;
  height: 200rpx;
}

.item_text {
  font-size: 32rpx;
  font-weight: bold;
  line-height: 65rpx;
  color:#333;

}

.bottom {
  width: 100%;
  padding: 0 0 30rpx 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  margin-top: 20rpx;
}

.name {
  font-size: 34rpx;
  font-weight: bold;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #E6E6E6;
}

.cell {
  padding: 20rpx 0;
  display: flex;
  border-bottom: 1rpx solid #E6E6E6;
}

.bottom_img {
  width: 130rpx;
  height: 130rpx;
}

.bottom_info {
  flex: 1;
  display: flex;
  flex-direction: column;
  padding: 0 20rpx;
}

.title {
  font-size: 34rpx;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.content {
  margin-top: 5rpx;
  font-size: 30rpx;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.mark {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  background-color: #000;
  opacity: 0.6;
  z-index: 1000;
}
.popup {
  width: 90%;
  background-color: #fff;
  z-index: 1001;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -300rpx;
  margin-left: -45%;
  border-radius: 20rpx;
  font-weight: 600;
  padding: 50rpx 20rpx;
  box-sizing: border-box;
  text-align: center;
}
.accreditButton {
  margin-top: 50rpx;
}

.list {
  padding: 20rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom: 1rpx solid #F3F3F3;
}
.avator {
  width: 170rpx;
  height: 170rpx;
  /* border-radius: 50%;   */
}
.right {
  flex: 1;
  padding: 20rpx 0 20rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between
}
.wux-rater {
  margin: 0
}
.score {
  display: flex;
  align-items: center;
}
.name {
  font-size: 32rpx;
  font-weight: 600;
}
.num {
  font-size: 28rpx;
  color: #999;
  margin-left: 10rpx;
}
.good_at {
  font-size: 32rpx;
}
.star_shan {
  color: #FFCC67;
}

.star_noshan {
  color: #ccc;
}

.star {
  display: flex;
  align-items: center;
}

.condition {
  padding: 20rpx 0;
  display: flex;
  color: #666;
  font-weight: 600;
  position: relative;
  background-color: #fff;
  margin-top: 20rpx;
}
.condition_item {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  box-sizing: border-box; 
}
.storefront {
  border-right: 1rpx solid #E6E6E6;
}
.storefront_select {
  color: #000;
  border-right: 1rpx solid #E6E6E6;
}
.classify {
  color: #000;
}
.storefront_content {
  width: 50%;
  position: absolute;
  top: 80rpx;
  left: 0;
  padding: 20rpx 0;
  background-color: #fff;
  z-index: 999;
  border-bottom: 1rpx solid #E6E6E6;
  border-left: 1rpx solid #E6E6E6;
  border-right: 1rpx solid #E6E6E6;
  box-sizing: border-box;
}
.storefront_content_item {
  margin: 0 auto;
  width: 250rpx;
  text-align: center;
  padding: 20rpx 0;
  color: #666;
  border-bottom: 1rpx solid #F3F2F2;
  font-size: 30rpx;
}
.storefront_content_item_ed {
  margin: 0 auto;
  width: 250rpx;
  text-align: center;
  padding: 20rpx 0;
  font-size: 30rpx;
  color: #FBCC08;
  border-bottom: 1rpx solid #FBCC08
}

.classify_content {
  width: 50%;
  position: absolute;
  top: 80rpx;
  right: 0;
  padding: 20rpx 0;
  background-color: #fff;
  z-index: 999;
  border-bottom: 1rpx solid #E6E6E6;
  border-left: 1rpx solid #E6E6E6;
  border-right: 1rpx solid #E6E6E6;
  box-sizing: border-box;
}
.classify_content_item {
  margin: 0 auto;
  width: 250rpx;
  text-align: center;
  padding: 20rpx 0;
  color: #666;
  border-bottom: 1rpx solid #F3F2F2;
  font-size: 30rpx;
}
.classify_content_item_ed {
  margin: 0 auto;
  width: 250rpx;
  text-align: center;
  padding: 20rpx 0;
  font-size: 30rpx;
  color: #FBCC08;
  border-bottom: 1rpx solid #FBCC08
}

.show_trainer {
  background-color: #fff;
  font-size: 34rpx;
  text-align: center;
  padding: 10rpx;
  margin-top: 20rpx;
}
.hide_trainer {
  background-color: #fff;
